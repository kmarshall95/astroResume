---
import Layouts from '../../layouts/BaseLayout.astro';
export async function getStaticPaths() {
  const posts = await Astro.glob('../webdev/*.{md,mdx}');
  return posts.map((post) => {
      return {
        params: {
          slug: post.title
        },
        props: {
          post
        },
      };
      
      
    
  });
   
}

const {Content, frontmatter} = Astro.props;

---
<Layouts>
    <Content/>

</Layouts>

